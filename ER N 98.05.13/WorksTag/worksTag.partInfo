//Filter.partInfoDefConst ( 'k_AuthorName, "Les R. Titze" );DefConst ( 'k_CopyrightStr,	LocObj ( "© 1997 Netstrategy Software", 'extrasSlip.copyString ) );partData := {	author:			k_AuthorName,	copyright:		k_CopyrightStr,	};constant kFilterVersion  := "1.4.2";constant kPartName       := "inet Copy to Works";constant kPartFrameTitle := kPartName;		// WAS: "EnRouteXTN:VFTagAct:LRT";DefConst ( 'k_BuildTime, Time() );OpenResFile( Home & "resources"); 	DefConst ( 'kLibIcon, GetPICTAsBits ( "tagExtnIcon", true ) );CloseResFile();DefConst ( 'k_yearMonthDayStrSpec, 180969 ); // (ROM_datetimestrspecs).yearMonthDayStrSpecDefConst ( 'k_AboutText,	LocObj ( "Created:", 'extrasSlip.created )	&& LongDateStr ( k_BuildTime, k_yearMonthDayStrSpec ) & unicodeCR	& LocObj ( "Author:", 'extrasSlip.author ) && k_AuthorName & unicodeCR	& k_CopyrightStr & unicodeCR	& "\nMail viewer \u0022\uaction tag\u0022\u for copying message text to Works documents from EnRoute i-net mail." & unicodeCR;);SetPartFrameSlot ( 'text, kPartName ); // give extras icon a better nameSetPartFrameSlot ( 'icon, kLibIcon );SetPartFrameSlot ( 'closeControl, true );SetPartFrameSlot ( 'TapAction, func()	begin	BuildContext ( {	    _proto: @180,	    viewJustify: 80,	    viewBounds: SetBounds ( 0, 0, 200, 130 ),	    stepChildren: [	      { _proto: @229,	        title: kPartName && kFilterVersion,	        },	      { _proto:		@218,	        text:			k_AboutText,	        viewJustify:	240,	        viewBounds:	SetBounds(5, 22, -5, -5),	        viewFont:	@100,	        },		]		} ) :Open();	nil;	end );print ( DateNTime(Time()) && "<---" && $' & kPartName & $' && "as '|" & kAppSymbol & "| Version" && kFilterVersion & (if kDebugOn then " DEBUG" else nil) );