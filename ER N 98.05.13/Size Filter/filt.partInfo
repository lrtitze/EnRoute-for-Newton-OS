//Filter.partInfoDefConst ( 'k_AuthorName, "Les R. Titze" );DefConst ( 'k_CopyrightStr,	LocObj ( "© 1997 Netstrategy Software", 'extrasSlip.copyString ) );partData := {	author:			k_AuthorName,	copyright:		k_CopyrightStr,	};//partData.RegEmail := kRegEmailSystem;constant kFilterVersion  := "1.4";constant kPartName       := LocObj ( "Message Size Filter", 'sizeFilter.extrasLabel );constant kPartFrameTitle := kPartName;print ( DateNTime(Time()) && "<---" && $' & kPartName & $' && "as '|" & kAppSymbol & "| Version" && kFilterVersion & (if kDebugOn then " DEBUG") );OpenResFile( Home & "resources"); 	DefConst ( 'kLibIcon, GetPICTAsBits ( "AddonFilterIcon", true ) );CloseResFile();SetPartFrameSlot ( 'text, kPartName ); // give extras icon a better nameSetPartFrameSlot ( 'icon, kLibIcon );SetPartFrameSlot ( 'closeControl, true );SetPartFrameSlot ( 'TapAction, func()	begin	local pkg := LFetch ( GetPackages(), kPartFrameTitle, 0, '|str=|, 'title );	local pkgE := GetPackageEntry ( kPartFrameTitle, pkg.store );	local info := GetPkgRefInfo(pkgE.pkgRef);	local s := LongDateStr ( info.creationdate, (ROM_datetimestrspecs).yearMonthDayStrSpec );	s :=  LocObj ( "Created:", 'extrasSlip.created ) && s & unicodeCR		& LocObj ( "Author:", 'extrasSlip.author ) && k_AuthorName & unicodeCR		& k_CopyrightStr & unicodeCR;	s := s & unicodeCR & LocObj (		"A simple filter extension for the EnRoute internet transport.",		'sizeFilter.extrasDescrip );	local height := 125;	local a := BuildContext ( {	    _proto: @180,	    viewJustify: 80,	    viewBounds: SetBounds ( 0, 0, 200, height ),	    stepChildren: [	      { _proto: @229,	        title: kPartName && kFilterVersion,	        },	      { _proto: @218,	        text: s,	        viewJustify: 240,	        viewBounds: SetBounds(5, 22, -5, -5),	        viewFont: @100,	        },		]		} );	a:Open();//	GetRoot():Notify ( kNotifyAlert+1, kPartName && kFilterVersion, s );	return nil;	end );