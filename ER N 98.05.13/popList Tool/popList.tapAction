//SetPartFrameSlot ( 'TapAction, k_InfoAboutFunc );DefConst ( 'k_OpenViewOrAboutbox, func()	begin	local plmView := GetGlobalVar ( kAppSymbol );	local tp := GetRoot().|EnRouteN1:Netstrategy|;	if plmView and tp then		begin		local v := BuildContext (			{			_proto:		plmView,			prefsView:	nil,			} );		v:Open();		end;	else		begin		call k_InfoAboutFunc with ();		end;	nil;	end );SetPartFrameSlot ( 'TapAction, k_OpenViewOrAboutbox );