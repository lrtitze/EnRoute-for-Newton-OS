DefConst ( 'k_DEMO_VERSION, true );strBanner := "THIS MESSAGE WAS SENT USING ENROUTE I-NET SOFTWARESUPPLIED TO NEWTON INC. FOR BETA TESTING.CONTACT NETSTRATEGY SOFTWARE FOR UPDATES.THIS ENROUTE TEST DEMO EXPIRES OCTOBER 1, 1997";binBanner := MakeBinary(Strlen(strBanner)+1,'binary);StuffCString ( binBanner,0,strBanner );xorByteVal := 0x15;for n := 0 to StrLen(strBanner)-1 do	begin			if xorByteVal MOD 7 = 0 then				xorByteVal := xorByteVal + 1;	StuffByte(binBanner,n,BXOr(xorByteVal,ExtractByte(binBanner,n)));	end;DefConst ( 'k_BIN_DEMO_BANNER, binBanner );DefConst ( 'k_DEMO_BANNER_MARK,"**************************************************" );