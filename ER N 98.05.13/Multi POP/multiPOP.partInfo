//Filter.partInfoDefConst ( 'k_AuthorName, "Les R. Titze" );DefConst ( 'k_CopyrightStr,	LocObj ( "© 1997 Netstrategy Software", 'extrasSlip.copyString ) );partData := {	author:			k_AuthorName,	copyright:		k_CopyrightStr,	};constant kFilterVersion  := "1.4.2";constant kPartName       := LocObj ( "inet Multi-POP", 'multiPOP.extrasLabel );constant kPartFrameTitle := kPartName;DefConst ( 'k_BuildTime, Time() );OpenResFile( Home & "resources"); 	DefConst ( 'kLibIcon, GetPICTAsBits ( "PrefExtnIcon", true ) );		// "AddonIcon"CloseResFile();SetPartFrameSlot ( 'text, kPartName ); // give extras icon a better nameSetPartFrameSlot ( 'icon, kLibIcon );SetPartFrameSlot ( 'closeControl, true );DefConst ( 'k_InfoAboutFunc, func()	begin	local s :=  LocObj ( "Created:", 'extrasSlip.created )		&& LongDateStr ( k_BuildTime, (ROM_datetimestrspecs).yearMonthDayStrSpec ) & unicodeCR		& LocObj ( "Author:", 'extrasSlip.author ) && k_AuthorName & unicodeCR		& k_CopyrightStr & unicodeCR		& unicodeCR & LocObj (		"Multiple POP account enabler for the EnRoute internet transport.",		'multiPOP.extrasDescrip );	local a := BuildContext ( {	    _proto: @180,	    viewJustify: 80,	    viewBounds: SetBounds ( 0, 0, 200, 115 ),	    stepChildren: [	      { _proto: @229,	        title: kPartName && kFilterVersion,	        },	      { _proto: @218,	        text: s,	        viewJustify: 240,	        viewBounds: SetBounds(5, 22, -5, -5),	        viewFont: @100,	        },		]		} );	a:Open();	nil;	end );SetPartFrameSlot ( 'TapAction, k_InfoAboutFunc );print ( DateNTime(Time()) && "<---" && $' & kPartName & $' && "as '|" & kAppSymbol & "| Version" && kFilterVersion );