InstallScript := func(partFrame)	begin	RegUnionSoup ( kAppSymbol, k_infoSoupDef );	RegAppClasses ( kAppSymbol, [ 'EnRouteRulesetData ] );	// new from Apple Q&A	local mainLayout := partFrame.theForm;	local defSym, defFrame, vDef;	if mainLayout.allViewDefs then		foreach defSym, defFrame in mainLayout.allViewDefs do			foreach vDef in defFrame do				begin				RegisterViewDef ( vDef, EnsureInternal ( defSym ) );				end;	partFrame.removeFrame := mainLayout:NewtInstallScript(mainLayout);	end;RemoveScript := func(partFrame)	begin	UnRegUnionSoup ( k_ruleSoupName, kAppSymbol );	UnregAppClasses ( kAppSymbol );	(partFrame.removeFrame):NewtRemoveScript(partFrame.removeFrame);	end;SetPartFrameSlot ( 'labels, '_setup );