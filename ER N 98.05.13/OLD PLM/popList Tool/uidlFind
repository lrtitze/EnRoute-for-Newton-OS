	local matchQuery := if thisLItem.UID then		{		validTest: func(a)			begin			local b := thisLItem;			a.UID			and			StrExactCompare ( a.UID, b.UID ) = 0;			end,		};	else		// no possible quick search		{		validTest: func(a)			begin			// (no subjects or Same Subjects)			//      AND			// (no Senders or Same Senders)			//      AND			// same Date			local b := thisLItem;			( (not (a.subj and b.subj)) or ( StrExactCompare(a.subj, b.subj) = 0 ) )			and			  (not (a.from and b.from) or ( StrExactCompare(a.from, b.from) = 0 ) )			and			  (a.when and (a.when = b.when) )			end;		};	local userUIDLSym := Intern ( fromAddress );	matchQuery.tagSpec := { all: userUIDLSym };	local cursor := _v_popSoup:Query ( matchQuery );	local matchItem := cursor:Entry();