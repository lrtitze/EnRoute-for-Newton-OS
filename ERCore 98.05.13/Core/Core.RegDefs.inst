DefConst ( 'k_InstallDefsFunc, func ( partFrame, removeFrame )	begin	// ==================================	//	MAIL TEXT DATA AND VIEW DEFS	// ==================================	local mailTxtSym := EnsureInternal ( kERMailTxtSym );	RegDataDef ( mailTxtSym, GetLayout ( "Cdd_mailText" ) );	// LRT - 98/01/15 1:43 AM	// OS 2.0 and greater can have extra stuff//	if k_OS_2_1_ONLY_VERSION then		RegisterViewDef ( GetLayout ( "Cvd_mHdrsVDef" ), mailTxtSym );	// kERMailTxtView//	else//		RegisterViewDef ( GetLayout ( "Cvd_mTextVDef" ), mailTxtSym );	// kERMailTxtView	RegisterViewDef ( k_mailTextVDef_prFrameFormat, mailTxtSym );	// 'prFrameFormat	RegisterViewDef ( k_mailTextVDef_mailFormat, mailTxtSym );	// 'mailFormat	// ==================================	//	INVALID ADDRESS VIEW DEF	// ==================================	RegisterViewDef ( GetLayout ( "Cvd_invalAddress" ), EnsureInternal(kInvAddressType) );	// 'invalidAddress = kInvAddrVDefSym	// ==================================	//	ROUTEBINARY VIEW DEF	// ==================================	RegisterViewDef ( k_RouteBinaryVDef, kRouteBinarySym );	RegisterViewDef ( k_RouteMIMEVDef, 'enRouteMIME );	// ==================================	//	ENROUTE GLOBALS SLOT	// ==================================	if not GetGlobalVar ( 'EnRouteCore ) then	// was 'EnRouteSystem		begin		local enRouteGlobs := EnsureInternal(			{			//	MacBinaryRegistry:	[],				installed:			[],				extended:			[],			} );		DefGlobalVar ( EnsureInternal('EnRouteCore), enRouteGlobs );		end;	if kDebugOn and k_DebugInstall then write ( "removeFrame: " );	if kDebugOn and k_DebugInstall then print ( removeFrame );	end );/********* USED IN-LINE IN ".inst" *******************DefConst ( 'k_RemoveDefsFunc, func ( removeFrame )	begin	// ==================================	//	MAIL TEXT DATA AND VIEW DEFS	// ==================================	UnregDataDef ( kERMailTxtSym );	UnregisterViewDef ( kERMailTxtView, kERMailTxtSym );	// 'enRouteMailTextView	UnregisterViewDef ( 'prFrameFormat, kERMailTxtSym );	UnregisterViewDef ( 'mailFormat, kERMailTxtSym );	UnregisterViewDef ( kInvAddrVDefSym, kInvAddressType );		// 'invalidAddress	UnRegisterViewDef ( kRouteBinarySym, kRouteBinarySym );	end );***************************************************/