/****************************************************************DefConst ( 'kStdFunc_Transport_AppClosed, func ( appSymbol )	begin	if enRoute and enRoute.connected and enRoute.mode = 'interactive then		:Confirm ( title,			ParamStr (				LocObj ( "Staying connected to the ^0 server will affect the amount of available memory.\n\nDo you want to disconnect now?", 'connect.stayConnMsg ),				[ title ] ),			self, 'ConfirmDisconnect );	end );DefConst ( 'kStdFunc_Transport_AppInFront, func ( inFront,appSymbol )	begin	if not inFront then :AppClosed ( appSymbol );	end );DefConst ( 'kStdFunc_Transport_CloseStatusDialog, func(fromUser)	begin	if enRoute	and enRoute.connected	and enRoute.tickle_backgrounder then		begin		enRoute.tickle_time := 0;		enRoute:ER_TicklePostNow();		end;	inherited:CloseStatusDialog ( fromUser )	end );****************************************************************/