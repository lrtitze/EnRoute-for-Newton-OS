// LRT - 96/12/13 4:04 AM// Don't allow installation on a cardSetPartFrameSlot ( 'DoNotInstall, func()	begin	if GetStores()[0] <> GetVBOStore(ObjectPkgRef('foo)) then		begin		GetRoot():Notify ( kNotifyAlert, kAppName,			LocObj ( "Installation failed.\n\nThe EnRoute Core can be installed ONLY onto the internal store.",			'errors.notOnCard ) );		true;		end;	else if k_OS_2_1_ONLY_VERSION then		begin		if Gestalt(0x1000003).ROMVersion < 131074 then		begin		GetRoot():Notify ( kNotifyAlert, kAppName,			LocObj ( "Unable to install on this hardware.\n\nPlease contact Netstrategy Software for a version which will work with your Newton.",			'errors.newNewtOnly ) );		true;		end;		end;	end );InstallScript := func ( partFrame, removeFrame )	begin	call k_InstallDefsFunc with ( partFrame, removeFrame );	if kDebugOn and k_DebugPartFrame then DefGlobalVar ( 'a, partFrame );	end;RemoveScript := func ( removeFrame )	begin	if kDebugOn and k_DebugPartFrame then UndefGlobalVar ( 'a );	if kDebugOn and k_DebugInstall then print ( removeFrame );	//call k_RemoveDefsFunc with ( removeFrame );	UnregDataDef ( kERMailTxtSym );	UnregisterViewDef ( kERMailTxtView, kERMailTxtSym );	// 'enRouteMailTextView	UnregisterViewDef ( 'prFrameFormat, kERMailTxtSym );	UnregisterViewDef ( 'mailFormat, kERMailTxtSym );	UnregisterViewDef ( kInvAddrVDefSym, kInvAddressType );		// 'invalidAddress	UnRegisterViewDef ( kRouteBinarySym, kRouteBinarySym );	UnRegisterViewDef ( 'enRouteMIME, 'enRouteMIME );	/********************	AddDelayedCall ( func()		begin		// scan the array of installed transports and freeze them		// if possible		end, [], 5000 );	*********************/	end;/***************************************************************DeletionScript := func()	begin	end;***************************************************************/